<%@page import="admin.AjaxController"%>
<%@page contentType="text/html" pageEncoding="UTF-8"%>
<%@page session="false" %>

<!DOCTYPE html>
<html>
    <head>
        <%
            String[] creds = AjaxController.parseCookies(request.getCookies());
            if (creds[0] == null || creds[1] == null) {
                response.sendRedirect("auth.jsp");
            }
        %>
        <style>
            @media screen and (max-width: 768px) {
                #sidebar {
                    background-color: #008800;
                }
            }
        </style>
        <title>WebMack</title>
        <link rel="stylesheet" type="text/css" href="global.css"/>
        <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
        <script type="text/javascript">$.ajax({type:'POST', data:{clickSource:'begin'}, url:'AjaxController'});</script>
        <script type="text/javascript">
            function ani() {
            var snd2 = new Audio("data:audio/wav;base64,");
            snd2.play();
            }
        </script>
        <script type="text/javascript">
            $(document).ready(function () {
            $('#btnHorarios').click(function () {$.ajax({type: 'POST', data: {clickSource: 'horarios'}, url: 'AjaxController', success: function (result) {$('#result1').html(result); }}); });
            $('#btnFaltas').click(function () {$.ajax({type: 'POST', data: {clickSource: 'faltas'}, url: 'AjaxController', success: function (result) {$('#result1').html(result); }}); });
            $('#btnNotas').click(function () {$.ajax({type: 'POST', data: {clickSource: 'notas'}, url: 'AjaxController', success: function (result) {$('#result1').html(result); }}); });
            $('#btnTarefas').click(function () {$.ajax({type: 'POST', data: {clickSource: 'tarefas'}, url: 'AjaxController', success: function (result) {$('#result1').html(result); }}); });
            $('#btnComidas').click(function () {$.ajax({type: 'POST', data: {clickSource: 'comidas'}, url: 'AjaxController', success: function (result) {$('#result1').html(result); }}); });
            $('#btnSuporte').click(function () {$.ajax({type: 'POST', data: {clickSource: 'suporte'}, url: 'AjaxController', success: function (result) {$('#result1').html(result); }}); });
            $('#btnAtt').click(function () {$.ajax({type: 'POST', data: {clickSource: 'att'}, url: 'AjaxController', success: function (result) {$('#result1').html(result); }}); });
            $('#btnConfig').click(function () {$.ajax({type: 'POST', data: {clickSource: 'config'}, url: 'AjaxController', success: function (result) {$('#result1').html(result); }}); });
            });
        </script>
    </head>
    <div id="ragu" style="position:absolute"></div>
    <div id="header">
        <div class="logo">
            <a href="#">
                Web<span>Mack</span>
            </a>
        </div>
    </div>
    <body>
        <div id="container">
            <div class="sidebar">
                <ul id="nav">
                    <li id="btnHorarios" class="horarios-ico"><a>Horários</a></li>
                    <li id="btnFaltas" class="faltas-ico"><a>Faltas*</a></li>
                    <li id="btnNotas" class="notas-ico"><a>Notas*</a></li>
                    <li id="btnTarefas" class="tarefas-ico"><a>Tarefas*</a></li>
                    <li id="btnComidas" class="comidas-ico"><a>Comidas/Delivery*</a></li>
                    <li id="btnSuporte" class="suporte-ico"><a>Suporte*</a></li>
                    <li id="btnAtt" class="att-ico" onclick="ani()"><a>Atualizar*</a></li>
                    <li id="btnConfig" class="config-ico"><a>Configurações*</a></li>
                </ul>
                <form action="esquecer.jsp" method="POST">
                    <input class="forget" type="submit" value="Esquecer de mim">
                </form>
                *: not implemented yet!
            </div>
            <div class="content">
                <span id="result1"></span>
            </div>
        </div>
    </body>
</html>
